#include<stdio.h>
int main()
{
    printf("1.sandwich\n2.french fries\n3.pasta\n4.burger\n5.pizza\n");
    printf("enter a choice:");
    int choice=0;
    scanf("%d",&choice);
    switch(choice)
    {
        case 1:printf("Food item-sandwich\nPrice-Rs.149");
               break;
        case 2:printf("Food item-french fries\nPrice-Rs.99");
               break;
        case 3:printf("Food item-pasta\nPrice-Rs.179");
               break;
        case 4:printf("Food item-burger\nPrice-Rs.129");
               break;
        case 5:printf("Food item-pizza\nPrice-Rs.239");
               break;
        default:printf("Invalid choice");       
    }
    return 0;
}
